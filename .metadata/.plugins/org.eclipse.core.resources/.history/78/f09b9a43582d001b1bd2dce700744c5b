package com;

public class AlternateClass {
	
	public static void main(String args[]) {
		int[] numArr = {5,2,8,7,4,3,9};
		int max = 0 , min = numArr[0];
		
		for(int i = 0 ; i < numArr.length ; i++) {
			if(numArr[i] > max) {
				max = numArr[i];
			}
			if(numArr[i] < min) {
				min = numArr[i];
			}
		}
		
		System.out.print(max + " " + min);
	}
}
